  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "postinstall": "husky install",
    "build:electron": "react-app-rewired build",
    "serve:electron": "concurrently \"BROWSER=none npm start\" \"npm:electron:dev\"",
    "electron:dev": "wait-on http://localhost:3000 && electron .",
    "package:mac": "npm run build:electron && npx electron-builder build --config ./electron/electron-builder.json  --macos --x64 --publish always",
    "package:linux": "npm run build:electron && npx electron-builder build --config ./electron/electron-builder.json --linux --x64 --publish always",
    "package:windows": "npm run build:electron && npx electron-builder build --config ./electron/electron-builder.json --win --ia32 --x64 --publish always",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.2",
    "pretty-quick": "^3.1.3",
    "react-app-rewired": "^2.2.1",
    "redux-saga": "^1.3.0",
    "redux-thunk": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format --staged"
    }
  },
  "lint-staged": {
    "**/*.+(js|jsx|json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
