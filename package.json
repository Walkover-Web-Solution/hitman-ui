    "npm": "6.0.0"
  },
  "scripts": {
    "start": "standard && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "postinstall": "husky install",
    "fix": "standard --fix",
    "build:electron": "react-app-rewired build",
    "serve:electron": "concurrently \"BROWSER=none npm start\" \"npm:electron:dev\"",
    "electron:dev": "wait-on http://localhost:3000 && electron .",
    "package:mac": "npm run build:electron && npx electron-builder build --config ./electron/electron-builder.json  --macos --x64",
    "package:linux": "npm run build:electron && npx electron-builder build --config ./electron/electron-builder.json --linux --x64",
    "package:windows": "npm run build:electron && npx electron-builder build --config ./electron/electron-builder.json --win --ia32 --x64 "
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^13.1.1",
    "electron-builder": "^22.10.5",
    "husky": "^5.0.1",
    "react-app-rewired": "^2.1.8",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "standard": "^16.0.3"
  },
  "standard": {
    "ignore": [
      "registerServiceWorker.js",
      "openApiForm.jsx"
    ],
    "parser": "babel-eslint"
  }
}
